{
  "name": "high_resolution_image_workload",
  "description": "High resolution image workload",
  "duration_sec": 30,

  "backend": {
    "type": "rerun",
    "server": {
      "port": 9876
    }
  },

  "clients": [
    {
      "language": "python",
      "count": 4,
      "data": {
        "type": "image",
        "size": "3000x3000"
      },
      "frequency_hz": 10
    }
  ],

  "metrics": {
    "server": {
      "memory_sample_interval_ms": 100
    },
    "output_dir": "./results"
  }
}
